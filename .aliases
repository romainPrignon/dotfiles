# Easier navigation: .., ..., ...., ....., ~ and -
alias -="cd -"
alias ~="cd ~"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Shortcuts
alias d="cd ~/Documents"
alias dl="cd ~/Téléchargements"
alias w="cd ~/workspace"
alias g="git"
alias h="history"
